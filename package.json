{
  "name": "karakum",
  "version": "1.0.0-alpha.20",
  "description": "Converter of TypeScript declaration files to Kotlin declarations",
  "keywords": [
    "kotlin",
    "typescript"
  ],
  "license": "Apache-2.0",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "bin": "./build/cli.js",
  "type": "module",
  "files": [
    "LICENSE",
    "README.md",
    "build/"
  ],
  "scripts": {
    "clean": "rimraf build",
    "build": "tsc --project tsconfig.build.json",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "ts-node --esm test/sandbox/test.ts"
  },
  "dependencies": {
    "glob": "^10.3.3",
    "minimatch": "^9.0.3",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^20.4.2",
    "@types/yargs": "^17.0.22",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1"
  },
  "peerDependencies": {
    "typescript": "^5.1.6"
  }
}
